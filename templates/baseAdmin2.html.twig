        asset('Admin/assets/vendor/echarts/echarts.min.js')
      }}"></script>
    <script src="{{
        asset('Admin/assets/vendor/quill/quill.min.js')
      }}"></script>
    <script src="{{
        asset('Admin/assets/vendor/simple-datatables/simple-datatables.js')
      }}"></script>
    <script src="{{
        asset('Admin/assets/vendor/tinymce/tinymce.min.js')
      }}"></script>
    <script src="{{
        asset('Admin/assets/vendor/php-email-form/validate.js')
      }}"></script>

    <!-- Template Main JS File -->
    <script src="{{ asset('Admin/assets/js/main.js') }}"></script>

    {% endblock %}
  </head>

